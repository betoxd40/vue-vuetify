  <v-container v-if=\"isLoading\">\n    <div class=\"text-center\">\n      <v-progress-circular\n        indeterminate\n        :size=\"150\"\n        :width=\"8\"\n        color=\"green\"\n      ></v-progress-circular>\n    </div>\n  </v-container>\n  <v-container v-else grid-list-xl>\n    <v-layout wrap>\n      <v-flex xs4 v-for=\"(item, index) in apiResponse\" :key=\"index\" mb-2>\n        <v-card>\n          <v-img :src=\"item.Poster\" aspect-ratio=\"1\"></v-img>\n          <v-card-title primary-title>\n            <div>\n              <h2>{{ item.Title }}</h2>\n              <div>Year: {{ item.Year }}</div>\n              <div>Type: {{ item.Type }}</div>\n              <div>IMDB-id: {{ item.imdbID }}</div>\n            </div>\n          </v-card-title>\n          <v-card-actions class=\"justify-center\">\n            <v-btn color=\"green\" @click=\"goToMovie(item.imdbID)\">View</v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-flex>\n    </v-layout>\n  </v-container>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n  data() {\n    return {\n      apiResponse: [],\n      isLoading: true,\n      error: false\n    }\n  },\n  async mounted() {\n    this.getApiInfo()\n  },\n  methods: {\n    getApiInfo: async function() {\n      try {\n        const res = await axios.get(\n          `http://www.omdbapi.com/?s=mummy&apikey=b76b385c&page=1&type=movie&Content-Type=application/json`\n        )\n        this.apiResponse = res.data.Search\n        this.isLoading = false\n      } catch (error) {\n        console.log(error)\n        this.error = true\n        this.isLoading = false\n      }\n    },\n    goToMovie: function(id) {\n      this.$router.push('/movie/' + id)\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views"}]}