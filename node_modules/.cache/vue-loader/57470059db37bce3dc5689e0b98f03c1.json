em.imdbID }}</div>\n            </div>\n          </v-card-title>\n\n          <v-card-actions class=\"justify-center\">\n            <v-btn flat color=\"green\" @click=\"goToMovie(item.imdbID)\"\n              >View</v-btn\n            >\n          </v-card-actions>\n        </v-card>\n      </v-flex>\n    </v-layout>\n  </v-container>\n</template>\n\n<script>\nimport axios from 'axios'\nexport default {\n  props: {\n    name: {\n      type: String,\n      required: true\n    }\n  },\n  data() {\n    return {\n      apiResponse: [],\n      isLoading: true,\n      error: false\n    }\n  },\n  mounted() {\n    this.getApiInfo(this.name)\n  },\n  watch: {\n    name(value) {\n      this.getApiInfo(value)\n    }\n  },\n  methods: {\n    getApiInfo: async function(value) {\n      try {\n        const res = await axios.get(\n          `http://www.omdbapi.com/?apikey=b76b385c&s=${value}&Content-Type=application/json%27`\n        )\n        console.log(res)\n        this.apiResponse = res.data.Search\n        this.isLoading = false\n      } catch (error) {\n        console.log(error)\n        this.error = true\n        this.isLoading = false\n      }\n    },\n    goToMovie(id) {\n      this.$router.push('/movie/' + id)\n    }\n  }\n}\n</script>\n\n<style></style>\n"]}]}